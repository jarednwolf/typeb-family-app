{
  "name": "typeb-family-app",
  "version": "1.1.0",
  "main": "index.ts",
  "scripts": {
    "start": "expo start",
    "android": "expo run:android",
    "ios": "expo run:ios",
    "web": "expo start --web",
    "postinstall": "patch-package",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:unit": "jest --testPathPattern=unit",
    "test:integration": "jest --config jest.config.integration.js",
    "lint": "eslint . --ext .ts,.tsx",
    "type-check": "tsc --noEmit",
    "emulators:start": "firebase emulators:start",
    "emulators:start:import": "firebase emulators:start --import=./emulator-data",
    "emulators:export": "firebase emulators:export ./emulator-data",
    "emulators:exec": "firebase emulators:exec",
    "seed:data": "ts-node src/test-utils/seed-test-data.ts",
    "test:emulators": "firebase emulators:exec 'npm test'",
    "e2e:seed": "node scripts/seed-emulator.js",
    "e2e:emulators:start": "firebase emulators:start --only auth,firestore,storage",
    "e2e:build:ios": "EXPO_PUBLIC_E2E_TEST=true detox build --configuration ios.sim.debug",
    "e2e:build:android": "EXPO_PUBLIC_E2E_TEST=true detox build --configuration android.emu.debug",
    "e2e:test:ios": "EXPO_PUBLIC_E2E_TEST=true detox test --configuration ios.sim.debug",
    "e2e:test:android": "EXPO_PUBLIC_E2E_TEST=true detox test --configuration android.emu.debug",
    "e2e:ios": "npm run e2e:build:ios && npm run e2e:test:ios",
    "e2e:android": "npm run e2e:build:android && npm run e2e:test:android",
    "e2e:ios:emulator": "firebase emulators:exec --only auth,firestore,storage 'npm run e2e:seed && npm run e2e:test:ios'",
    "e2e:android:emulator": "firebase emulators:exec --only auth,firestore,storage 'npm run e2e:seed && npm run e2e:test:android'",
    "e2e:run": "./scripts/run-e2e-tests.sh",
    "e2e:run:auth": "./scripts/run-e2e-tests.sh ios auth",
    "e2e:run:family": "./scripts/run-e2e-tests.sh ios family",
    "e2e:run:tasks": "./scripts/run-e2e-tests.sh ios tasks",
    "perf:generate": "ts-node scripts/performance-test-data.ts generate",
    "perf:cleanup": "ts-node scripts/performance-test-data.ts cleanup",
    "perf:test:small": "ts-node scripts/performance-test-data.ts generate 10 100 30 --recurring --photos",
    "perf:test:medium": "ts-node scripts/performance-test-data.ts generate 15 200 60 --recurring --photos --overdue",
    "perf:test:large": "ts-node scripts/performance-test-data.ts generate 20 500 150 --recurring --photos --overdue",
    "security:test": "ts-node scripts/run-security-tests.ts",
    "security:pentest": "ts-node scripts/run-security-tests.ts --comprehensive",
    "security:test:auth": "ts-node scripts/run-security-tests.ts --category=authentication",
    "security:test:authz": "ts-node scripts/run-security-tests.ts --category=authorization",
    "security:test:injection": "ts-node scripts/run-security-tests.ts --category=injection",
    "security:audit": "npm audit && npm outdated",
    "security:fix": "npm audit fix"
  },
  "dependencies": {
    "@expo/vector-icons": "^14.0.0",
    "@react-native-async-storage/async-storage": "1.21.0",
    "@react-native-community/datetimepicker": "7.7.0",
    "@react-native-community/netinfo": "11.1.0",
    "@react-navigation/bottom-tabs": "^6.5.11",
    "@react-navigation/native": "^6.1.9",
    "@react-navigation/stack": "^6.3.20",
    "@reduxjs/toolkit": "^2.0.1",
    "date-fns": "^4.1.0",
    "expo": "~50.0.6",
    "expo-background-fetch": "^13.1.6",
    "expo-camera": "~14.0.4",
    "expo-constants": "~15.4.5",
    "expo-device": "~5.9.3",
    "expo-image-picker": "~14.7.1",
    "expo-notifications": "~0.27.6",
    "expo-splash-screen": "~0.26.4",
    "expo-status-bar": "~1.11.1",
    "expo-task-manager": "^13.1.6",
    "firebase": "^10.7.1",
    "react": "18.2.0",
    "react-native": "0.73.4",
    "react-native-confetti-cannon": "^1.5.2",
    "react-native-gesture-handler": "~2.14.0",
    "react-native-get-random-values": "^1.11.0",
    "react-native-launch-arguments": "^4.1.0",
    "react-native-purchases": "^9.2.0",
    "react-native-reanimated": "~3.6.2",
    "react-native-safe-area-context": "4.8.2",
    "react-native-screens": "~3.29.0",
    "react-redux": "^9.0.4"
  },
  "devDependencies": {
    "@babel/core": "^7.20.0",
    "@config-plugins/detox": "^7.0.0",
    "@testing-library/jest-native": "^5.4.3",
    "@testing-library/react-native": "^12.4.3",
    "@types/jest": "^29.5.11",
    "@types/react": "~18.2.45",
    "@types/react-native": "^0.73.0",
    "@typescript-eslint/eslint-plugin": "^6.17.0",
    "@typescript-eslint/parser": "^6.17.0",
    "detox": "^20.13.5",
    "eslint": "^8.56.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-react": "^7.33.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "firebase-admin": "^13.4.0",
    "jest": "^29.7.0",
    "jest-circus": "^30.0.5",
    "jest-expo": "~50.0.1",
    "patch-package": "^8.0.0",
    "prettier": "^3.1.1",
    "react-test-renderer": "^18.2.0",
    "sharp": "^0.34.3",
    "ts-jest": "^29.1.1",
    "ts-node": "^10.9.2",
    "typescript": "^5.3.0"
  },
  "private": true
}
