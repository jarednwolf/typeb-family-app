{
  "name": "typeb-family-app",
  "version": "1.1.0",
  "private": true,
  "main": "node_modules/expo/AppEntry.js",
  "scripts": {
    "start": "expo start",
    "start:e2e": "expo start --no-dev --minify",
    "android": "expo start --android",
    "ios": "expo start --ios",
    "web": "expo start --web",
    "prebuild": "expo prebuild",
    "prebuild:clean": "expo prebuild --clean",
    "test": "jest",
    "test:unit": "USE_FIREBASE_EMULATOR=false jest --testPathIgnorePatterns='integration|e2e' --maxWorkers=4",
    "test:integration": "firebase emulators:exec --only auth,firestore,storage --project typeb-family-test 'jest --testPathPattern=integration --runInBand'",
    "test:e2e": "detox test --configuration ios.sim.debug",
    "test:e2e:android": "detox test --configuration android.emu.debug",
    "test:all": "npm run test:unit && npm run test:integration",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "lint": "eslint . --ext .js,.jsx,.ts,.tsx",
    "type-check": "tsc --noEmit",
    "eas:build:preview": "eas build --profile preview",
    "eas:build:production": "eas build --profile production",
    "eas:submit": "eas submit",
    "build:e2e:ios": "detox build --configuration ios.sim.debug",
    "build:e2e:android": "detox build --configuration android.emu.debug",
    "emulator:start": "firebase emulators:start --only auth,firestore,storage --import=./test-data --export-on-exit",
    "emulator:start:test": "firebase emulators:start --only auth,firestore,storage --project typeb-family-test",
    "emulator:export": "firebase emulators:export ./test-data",
    "emulator:seed": "node scripts/seed-emulator.js",
    "security:rules:test": "firebase emulators:exec --only firestore 'npm run test:rules'",
    "test:rules": "jest --testMatch='**/*.rules.test.ts'",
    "postinstall": "patch-package"
  },
  "dependencies": {
    "@expo/vector-icons": "^14.0.0",
    "@gorhom/bottom-sheet": "^4.5.1",
    "@react-native-async-storage/async-storage": "1.23.1",
    "@react-native-community/netinfo": "11.3.1",
    "@react-native-google-signin/google-signin": "^11.0.0",
    "@react-native-picker/picker": "2.7.5",
    "@react-navigation/bottom-tabs": "^6.5.11",
    "@react-navigation/native": "^6.1.9",
    "@react-navigation/native-stack": "^6.9.17",
    "@react-navigation/stack": "^6.4.1",
    "@reduxjs/toolkit": "^2.0.1",
    "@sentry/react-native": "^5.20.0",
    "date-fns": "^3.0.6",
    "expo": "~51.0.17",
    "expo-av": "~14.0.6",
    "expo-camera": "~15.0.13",
    "expo-constants": "~16.0.2",
    "expo-contacts": "~13.0.4",
    "expo-dev-client": "~4.0.20",
    "expo-device": "~6.0.2",
    "expo-haptics": "~13.0.1",
    "expo-image": "~1.12.12",
    "expo-image-manipulator": "^13.1.7",
    "expo-image-picker": "~15.0.6",
    "expo-linear-gradient": "~13.0.2",
    "expo-linking": "~6.3.1",
    "expo-localization": "~15.0.3",
    "expo-media-library": "~16.0.4",
    "expo-notifications": "^0.28.19",
    "expo-secure-store": "~13.0.2",
    "expo-sensors": "~13.0.8",
    "expo-sms": "~12.0.1",
    "expo-splash-screen": "~0.27.5",
    "expo-status-bar": "~1.12.1",
    "expo-system-ui": "~3.0.7",
    "expo-updates": "~0.25.17",
    "firebase": "^10.7.1",
    "react": "18.2.0",
    "react-native": "0.74.2",
    "react-native-calendars": "^1.1303.0",
    "react-native-confetti-cannon": "^1.5.2",
    "react-native-gesture-handler": "~2.16.1",
    "react-native-gifted-chat": "^2.4.0",
    "react-native-modal": "^13.0.1",
    "react-native-progress": "^5.0.1",
    "react-native-purchases": "^7.4.0",
    "react-native-reanimated": "~3.10.1",
    "react-native-safe-area-context": "4.10.1",
    "react-native-screens": "3.31.1",
    "react-native-sound": "^0.11.2",
    "react-native-svg": "15.2.0",
    "react-native-vector-icons": "^10.0.3",
    "react-redux": "^9.0.4",
    "redux-persist": "^6.0.0"
  },
  "devDependencies": {
    "@babel/core": "^7.20.0",
    "@testing-library/react-native": "^12.4.3",
    "@types/jest": "^29.5.11",
    "@types/react": "~18.2.45",
    "@types/react-native-vector-icons": "^6.4.18",
    "@types/react-test-renderer": "^18.0.7",
    "@types/redux-mock-store": "^1.5.0",
    "@typescript-eslint/eslint-plugin": "^6.17.0",
    "@typescript-eslint/parser": "^6.17.0",
    "detox": "^20.17.0",
    "eslint": "^8.56.0",
    "eslint-config-expo": "^7.0.0",
    "eslint-plugin-react": "^7.33.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "firebase-admin": "^12.0.0",
    "firebase-tools": "^13.0.3",
    "jest": "^29.7.0",
    "jest-expo": "~51.0.1",
    "patch-package": "^8.0.0",
    "react-test-renderer": "18.2.0",
    "redux-mock-store": "^1.5.5",
    "ts-jest": "^29.1.1",
    "ts-node": "^10.9.2",
    "typescript": "~5.3.3"
  },
  "expo": {
    "install": {
      "exclude": [
        "react-native-safe-area-context"
      ]
    }
  },
  "overrides": {
    "react-refresh": "~0.14.0"
  },
  "resolutions": {
    "react-refresh": "~0.14.0"
  }
}
