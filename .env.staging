# TypeB Staging Environment Configuration
# This file contains staging environment variables
# NEVER commit actual secrets to git - use GitHub Secrets for CI/CD

# Environment
EXPO_PUBLIC_ENVIRONMENT=staging

# Firebase Configuration (Staging Project)
FIREBASE_PROJECT_ID=typeb-family-app-staging
FIREBASE_API_KEY=${STAGING_FIREBASE_API_KEY}
FIREBASE_AUTH_DOMAIN=typeb-family-app-staging.firebaseapp.com
FIREBASE_STORAGE_BUCKET=typeb-family-app-staging.appspot.com
FIREBASE_MESSAGING_SENDER_ID=${STAGING_FIREBASE_MESSAGING_SENDER_ID}
FIREBASE_APP_ID=${STAGING_FIREBASE_APP_ID}
FIREBASE_MEASUREMENT_ID=${STAGING_FIREBASE_MEASUREMENT_ID}

# Firebase Admin (Server-side only)
FIREBASE_ADMIN_PROJECT_ID=typeb-family-app-staging
FIREBASE_ADMIN_CLIENT_EMAIL=${STAGING_FIREBASE_ADMIN_CLIENT_EMAIL}
FIREBASE_ADMIN_PRIVATE_KEY=${STAGING_FIREBASE_ADMIN_PRIVATE_KEY}

# RevenueCat (Staging/Sandbox)
EXPO_PUBLIC_REVENUECAT_API_KEY_IOS=${STAGING_REVENUECAT_API_KEY_IOS}
EXPO_PUBLIC_REVENUECAT_API_KEY_ANDROID=${STAGING_REVENUECAT_API_KEY_ANDROID}
REVENUECAT_WEBHOOK_SECRET=${STAGING_REVENUECAT_WEBHOOK_SECRET}
REVENUECAT_API_KEY=${STAGING_REVENUECAT_API_KEY}

# Sentry Error Monitoring
EXPO_PUBLIC_SENTRY_DSN=${STAGING_SENTRY_DSN}
SENTRY_AUTH_TOKEN=${STAGING_SENTRY_AUTH_TOKEN}
SENTRY_ORG=typeb
SENTRY_PROJECT=typeb-staging

# Google OAuth (SSO)
GOOGLE_CLIENT_ID=${STAGING_GOOGLE_CLIENT_ID}
GOOGLE_CLIENT_SECRET=${STAGING_GOOGLE_CLIENT_SECRET}
EXPO_PUBLIC_GOOGLE_WEB_CLIENT_ID=${STAGING_GOOGLE_WEB_CLIENT_ID}
EXPO_PUBLIC_GOOGLE_IOS_CLIENT_ID=${STAGING_GOOGLE_IOS_CLIENT_ID}
EXPO_PUBLIC_GOOGLE_ANDROID_CLIENT_ID=${STAGING_GOOGLE_ANDROID_CLIENT_ID}

# Apple Sign In
APPLE_CLIENT_ID=com.typebapp.family.staging
APPLE_TEAM_ID=${APPLE_TEAM_ID}
APPLE_KEY_ID=${APPLE_KEY_ID}
APPLE_PRIVATE_KEY=${APPLE_PRIVATE_KEY}

# Push Notifications
FCM_SERVER_KEY=${STAGING_FCM_SERVER_KEY}
EXPO_PUBLIC_VAPID_KEY=${STAGING_VAPID_KEY}

# API Configuration
EXPO_PUBLIC_API_URL=https://staging-api.typebapp.com
EXPO_PUBLIC_WEB_URL=https://staging.typebapp.com

# Vercel (Web Deployment)
VERCEL_TOKEN=${VERCEL_TOKEN}
VERCEL_ORG_ID=${VERCEL_ORG_ID}
VERCEL_PROJECT_ID=${VERCEL_PROJECT_ID}

# GitHub Actions
GITHUB_TOKEN=${GITHUB_TOKEN}
FIREBASE_TOKEN=${FIREBASE_TOKEN}

# Expo / EAS Build
EXPO_TOKEN=${EXPO_TOKEN}
EAS_PROJECT_ID=${EAS_PROJECT_ID}

# Analytics
EXPO_PUBLIC_MIXPANEL_TOKEN=${STAGING_MIXPANEL_TOKEN}
EXPO_PUBLIC_GA_MEASUREMENT_ID=${STAGING_GA_MEASUREMENT_ID}

# Feature Flags (Staging - Testing Features)
EXPO_PUBLIC_FEATURE_GOOGLE_SSO=true
EXPO_PUBLIC_FEATURE_APPLE_SIGNIN=true
EXPO_PUBLIC_FEATURE_PUSH_NOTIFICATIONS=true
EXPO_PUBLIC_FEATURE_ANALYTICS=true
EXPO_PUBLIC_FEATURE_PREMIUM=true

# Support
SUPPORT_EMAIL=staging-support@typebapp.com
PREMIUM_SUPPORT_EMAIL=staging-premium@typebapp.com

# Rate Limiting (More lenient for testing)
RATE_LIMIT_REQUESTS_PER_MINUTE=200
RATE_LIMIT_PHOTO_UPLOADS_PER_HOUR=50

# Storage Limits
MAX_PHOTO_SIZE_MB=5
MAX_FAMILY_PHOTOS_GB=10
PHOTO_RETENTION_DAYS=90

# Performance
API_TIMEOUT_MS=15000
IMAGE_OPTIMIZATION_QUALITY=80

# Debug (Enabled for staging)
DEBUG=true
LOG_LEVEL=info

# COPPA Compliance
COPPA_PHOTO_RETENTION_DAYS=90
COPPA_CONSENT_EXPIRY_DAYS=365
COPPA_MIN_AGE=13

# Added by env validator (placeholders for missing keys)
EXPO_PUBLIC_FIREBASE_API_KEY=
EXPO_PUBLIC_FIREBASE_AUTH_DOMAIN=
EXPO_PUBLIC_FIREBASE_PROJECT_ID=
EXPO_PUBLIC_FIREBASE_STORAGE_BUCKET=
EXPO_PUBLIC_FIREBASE_MESSAGING_SENDER_ID=
EXPO_PUBLIC_FIREBASE_APP_ID=
