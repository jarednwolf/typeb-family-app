{
  "cli": {
    "version": ">= 5.0.0",
    "appVersionSource": "remote"
  },
  "build": {
    "development": {
      "developmentClient": true,
      "distribution": "internal",
      "ios": {
        "resourceClass": "m-medium"
      }
    },
    "preview": {
      "distribution": "internal",
      "ios": {
        "resourceClass": "m-medium"
      },
      "android": {
        "buildType": "apk"
      }
    },
    "production": {
      "ios": {
        "resourceClass": "m-medium",
        "autoIncrement": true,
        "image": "latest"
      },
      "android": {
        "autoIncrement": true
      },
      "env": {
        "EXPO_PUBLIC_ENVIRONMENT": "production"
      }
    },
    "production-ios": {
      "ios": {
        "resourceClass": "m-medium",
        "autoIncrement": true,
        "image": "latest"
      },
      "env": {
        "EXPO_PUBLIC_ENVIRONMENT": "production",
        "EXPO_PUBLIC_FEATURE_GOOGLE_SSO": "false",
        "EXPO_PUBLIC_FEATURE_APPLE_SIGNIN": "false",
        "EXPO_PUBLIC_FEATURE_PUSH_NOTIFICATIONS": "true"
      }
    }
  },
  "submit": {
    "production": {
      "ios": {
        "ascAppId": "6749812496"
      },
      "android": {
        "serviceAccountKeyPath": "./path-to-your-service-account-key.json",
        "track": "production"
      }
    }
  }
}