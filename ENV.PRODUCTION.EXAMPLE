# ===== TypeB Mobile App - Production Env (Example) =====
# Copy to .env.production and fill with real values

# Core environment
EXPO_PUBLIC_ENVIRONMENT=production
EAS_PROJECT_ID=

# Firebase Web SDK (used by app)
FIREBASE_API_KEY=
FIREBASE_AUTH_DOMAIN=
FIREBASE_PROJECT_ID=
FIREBASE_STORAGE_BUCKET=
FIREBASE_MESSAGING_SENDER_ID=
FIREBASE_APP_ID=
FIREBASE_MEASUREMENT_ID=

# RevenueCat
EXPO_PUBLIC_REVENUECAT_API_KEY_IOS=
EXPO_PUBLIC_REVENUECAT_API_KEY_ANDROID=

# Sentry
EXPO_PUBLIC_SENTRY_DSN=

# Google Sign-In (OAuth client IDs)
EXPO_PUBLIC_GOOGLE_WEB_CLIENT_ID=
EXPO_PUBLIC_GOOGLE_IOS_CLIENT_ID=
EXPO_PUBLIC_GOOGLE_ANDROID_CLIENT_ID=

# API endpoints
EXPO_PUBLIC_API_URL=
EXPO_PUBLIC_WEB_URL=

# Feature flags
EXPO_PUBLIC_FEATURE_GOOGLE_SSO=true
EXPO_PUBLIC_FEATURE_APPLE_SIGNIN=true
EXPO_PUBLIC_FEATURE_PUSH_NOTIFICATIONS=true
EXPO_PUBLIC_FEATURE_ANALYTICS=true
EXPO_PUBLIC_FEATURE_PREMIUM=true

# Note: EAS CLI auth uses EXPO_TOKEN (not read by the app). Provide it in CI/terminal env.
# EXPO_TOKEN=




